/**
 * Legacy transaction items event_type - for first Avro release then to be deprecated in favour
 * of `Transaction.avdl`
 */

@namespace("com.snowplowanalytics.snowplow.storage.avro")
protocol TransactionItemsLegacyProtocol {

	record TransactionItemsLegacy {
		string order_id;
		string sku;
		string name;
		string category;
		float unit_price;
		float quantity;	
	}

}