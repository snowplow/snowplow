{
	"$schema": "http://json-schema.org/schema#",
	"title": "com.snowplowanalytics/unstruct_event/1.0.0",	
	"description": "Schema for a Snowplow unstructured event",
	"type": "object",

	"properties": {

		"schema": {
			"type": "string",
			"pattern": "^[a-zA-Z0-9_.]+/[a-zA-Z0-9_.]+/[a-zA-Z0-9_.]+/[0-9]+-[0-9]+-[0-9]+$"
		},

		"data": {
			"type": "object"
		}
	},

	"required": ["schema", "data"],
	"additionalProperties": false	
}
